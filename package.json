{
  "name": "svelte-electron-tailwind-app",
  "author": "Anita Ruangrotsakun",
  "description": "MS Project -- Video Labeling Electron App",
  "private": true,
  "version": "0.0.0",
  "main": "src/electron/main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development concurrently \"npm run web:watch\" \"npm run electron:start\"",
    "web:watch": "vite",
    "electron:start": "electron .",
    "build": "vite build && electron-builder --publish never",
    "test": "vitest run src",
    "test:watch": "vitest src"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.0.3",
    "@testing-library/svelte": "^3.2.2",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron": "^22.0.3",
    "electron-builder": "^23.6.0",
    "jest-dom": "^4.0.0",
    "jsdom": "^21.1.0",
    "msw": "^1.1.0",
    "postcss": "^8.4.21",
    "svelte": "^3.54.0",
    "svelte-spa-router": "^3.3.0",
    "tailwindcss": "^3.2.4",
    "vite": "^4.0.0",
    "vitest": "^0.29.2"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "mockserver": "^3.1.1",
    "npm-fetch": "^0.0.9"
  }
}
